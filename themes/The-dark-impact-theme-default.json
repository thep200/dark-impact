    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.quoted.docstring.multi.python",
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#A3A3A3"
            }
        },
        {
            "name":"The dark impact default theme",
            "scope": [
                // --------------- JS anf TS -------------------
                "keyword.control.from.js",
                "keyword.control.import.js",
                "keyword.control.from.ts",
                "keyword.control.import.ts"
            ],
            "settings": {

                "foreground": "#59CE8F" }
        },
        {
            "name":"The dark impact default theme",
            "scope": [
                // --------------- Keyword -------------------
                "keyword.other.use.php",
                "keyword.other.namespace.php",
                "storage.type.class.php",
                "storage.modifier.extends.php",
                "storage.modifier.php",
                "storage.type.function.php",
                "constant.language.php",
                "keyword.other.phpdoc.php",
                "keyword.other.type.php",
                "keyword.other.class.php",
                "punctuation.section.embedded.begin.php"
            ],
            "settings": {
                "foreground": "#59CE8F"
            }
        },
        {
            "name":"The dark impact default theme",
            "scope": [
                // --------------- Entity name keyword -------------------
                "support.class.php",
                "entity.name.type.namespace.php",
                "entity.other.alias.php",
                "entity.name.type.class.php",
                "entity.other.inherited-class.php"
            ],
            "settings": {
                "foreground": "#00FF7F"
            }
        },
        {
            "name":"The dark impact default theme",
            "scope": [
                // --------------- Variable -------------------
                "punctuation.definition.variable.php",
                "keyword.control.return.php",
                "variable.other.php",
                "variable.language.this.php",
                "support.type.property-name.json.comments"
            ],
            "settings": {
                "foreground": "#AFB4FF"
            }
        },
        {
            "name":"The dark impact default theme",
            "scope": [
                // --------------- Keyword other -------------------
                "keyword.other.use-as.php"
            ],
            "settings": {
                "foreground": "#FFFFF0"
            }
        },
        {
            "name":"The dark impact default theme",
            "scope": [
                // --------------- PHP and Laravel -------------------
                "support.other.namespace.php",

                // --------------- HTML -------------------
                "entity.name.tag.html"
            ],
            "settings": {
                "foreground": "#50C878"
            }
        },
        {
            "name":"The dark impact default theme",
            "scope": [
                // --------------- JS and TS -------------------
                "punctuation.separator.comma.js",
                "punctuation.separator.label.js",
                "punctuation.separator.comma.ts",
                "punctuation.separator.label.ts",
                "punctuation.separator.key-value.js",
                "punctuation.separator.parameter.js",

                // --------------- PHP and Laravel -------------------
                "punctuation.separator.classes.php",
                "punctuation.separator.inheritance.php",

                // --------------- HTML -------------------
                "punctuation.separator.key-value.html",

                // --------------- CSS and SCSS -------------------
                "punctuation.definition.entity.css",
                "punctuation.separator.key-value.css",
                "punctuation.separator.list.comma.css",
                "punctuation.separator.delimiter.scss",
                "punctuation.separator.key-value.scss"
            ],
            "settings": {
                "foreground": "#FFFFF0"
            }
        },
        {
            "name":"The dark impact default theme",
            "scope": [
                // --------------- CSS and SCSS -------------------
                "support.constant.property-value.css"
            ],
            "settings": {
                "foreground": "#E0B0FF"
            }
        }
    ]
